@using demo.Models
@model SymptomModel

<style>
    body {
        background-image: url(/Content/Imagers/source1.gif);
        background-size: 80%;
    }
</style>

<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <script src="~/scripts/jquery-3.3.1.min.js"></script>
    <script src="~/scripts/bootstrap.min.js"></script>
</head>

@{
    ViewBag.Title = "tablesymptom";
}

<h2>tablesymptom</h2>

@using (Html.BeginForm())
{
    <table>
        @for (int i = 0; i < Model.symptomslist.Count; i++)
        {
            if (i % 1 == 0)
            {
                @:
                <tr></tr>

            }

            <tr>
                <td>
                    @Html.CheckBoxFor(x => x.symptomslist[i].IsCheked, new { @class = "custom-checkbox" })
                    <label>@Model.symptomslist[i].SymptomName</label>
                    @Html.HiddenFor(x => x.symptomslist[i].SymptomId)
                    @Html.HiddenFor(x => x.symptomslist[i].SymptomName)
                </td>
            </tr>


        }

    </table>

    <input type="submit" class="btn btn-info" value="Proceed" />
}